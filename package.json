        "watch": "nodemon --exec \"npm run start\"",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com"
    },
    "dependencies": {
        "botbuilder": "4.21.0",
        "restify": "11.1.0"
    },
    "devDependencies": {
        "@types/node": "18.18.8",
        "@types/restify": "8.5.9",
        "env-cmd": "10.1.0",
        "nodemon": "3.0.1",
        "shx": "0.3.4",
        "ts-node": "10.9.1",
        "typescript": "5.2.2"
    }
}
